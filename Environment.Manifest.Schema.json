{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"title": "Environment Manifest",
	"description": "Jot Environment Manifest",
	"type": "string",
	"properties": {
		"AppSettingValues": {
			"description": "Define app settings",
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"Name": {
						"type": "string"
					},
					"Value": {
						"type": "string"
					}
				},
				"required": ["Name", "Value"]
			},
			"minItems": 0,
			"uniqueItems": true
		}
	},
	"required": ["AppSettingValues"]
}